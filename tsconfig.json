{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "esModuleInterop": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "l-ng-otp-input": [
        "dist/l-ng-otp-input-repo"
      ],
      "l-ng-otp-input/*": [
        "dist/l-ng-otp-input-repo/*"
      ]
    },
    "useDefineForClassFields": false
  }
}